{
  "version": "7.0.0-*",
  "description": "SQLite data store for Entity Framework.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/entityframework"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "EntityFramework.Relational": "7.0.0-*",
    "Microsoft.Data.Sqlite": "1.0.0-*"
  },
  "compile": "..\\Shared\\*.cs",
  "namedResource": {
    "EntityFramework.Sqlite.SqliteStrings": "Properties/SqliteStrings.resx"
  },
  "resource": [ "Properties/EntityFramework.Sqlite.rd.xml" ],
  "frameworks": {
    "net451": { },
    "dotnet5.4": {
      "dependencies": {
        "System.IO.FileSystem": "4.0.1-*",
        "Microsoft.CSharp": "4.0.1-*"
      }
    },
    "netcore50": {
      "dependencies": {
        "System.IO.FileSystem": "4.0.0"
      }
    }
  }
}
